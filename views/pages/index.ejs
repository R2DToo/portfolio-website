    <section id="contact">
        <div class="container">
            <div class="card bg-primary">
                <div class="card-title">
                    <h2 class="text-uppercase text-center text-white mb-0 mt-3">Contact Me</h2>
                    <hr class="star-light mb-3">
                </div>
                <div class="card-body">
                    <form id="contactForm" action="/contact" method="POST">
                        <div class="form-group form-floating mb-0 pb-2">
                            <input class="form-control" type="text" id="name" name="name" placeholder="Name" required>
                            <label for="name">Name <sup>*</sup></label>
                        </div>
                        <div class="form-group form-floating mb-0 pb-2">
                            <input class="form-control" type="email" id="email" placeholder="Email" name="email" required>
                            <label for="email">Email <sup>*</sup></label>
                        </div>
                        <div class="form-group form-floating mb-0 pb-2">
                            <input class="form-control" type="tel" id="phone" placeholder="Phone Number" name="phone">
                            <label for="phone">Phone Number</label>
                        </div>
                        <div class="form-group form-floating mb-3 pb-2">
                            <textarea class="form-control" id="message" rows="5" placeholder="Message" name="message" required></textarea>
                            <label for="message">Message <sup>*</sup></label>
                        </div>
                        <div class="form-group">
                            <button class="btn btn-secondary btn-xl" id="sendMessageButton" type="submit">Send</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
